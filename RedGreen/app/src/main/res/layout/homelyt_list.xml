<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/baground"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/clean_memory"
        android:layout_width="match_parent"
        android:layout_height="@dimen/zero_dp"
        android:layout_marginBottom="@dimen/five_dp"
        android:layout_marginLeft="@dimen/five_dp"
        android:layout_marginRight="@dimen/five_dp"
        android:layout_marginTop="@dimen/ten_dp"
        android:layout_weight="1"
        android:background="@drawable/bg_list"
        android:visibility="gone" >

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_margin="@dimen/ten_dp"
            android:src="@drawable/clean_memory_list" />

        <ImageView
            android:id="@+id/cleanmemory_image_arrow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_margin="@dimen/ten_dp"
            android:src="@drawable/arrow_list" />
    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/cleanapp_image"
        android:layout_width="match_parent"
        android:layout_height="@dimen/zero_dp"
        android:layout_marginBottom="@dimen/five_dp"
        android:layout_marginLeft="@dimen/five_dp"
        android:layout_marginRight="@dimen/five_dp"
        android:layout_marginTop="@dimen/ten_dp"
        android:layout_weight="1"
        android:background="@drawable/bg_list" >

        <ImageView
            android:layout_width="@dimen/home_image_lit_dimen"
            android:layout_height="wrap_content"
            android:src="@drawable/clean_app_list"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="@dimen/thirty_dp"
            android:layout_marginTop="@dimen/thirty_dp"
            android:layout_marginBottom="@dimen/thirty_dp" />

        <ImageView
            android:id="@+id/cleanapp_image_arrow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_margin="@dimen/ten_dp"
            android:src="@drawable/arrow_list" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/datasecurity_image"
        android:layout_width="match_parent"
        android:layout_height="@dimen/zero_dp"
        android:layout_margin="@dimen/five_dp"
        android:layout_weight="1"
        android:background="@drawable/bg_list" >

        <ImageView
            android:layout_width="@dimen/home_image_lit_dimen"
            android:layout_height="wrap_content"
            android:src="@drawable/speed_boost"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="@dimen/thirty_dp"
            android:layout_marginTop="@dimen/thirty_dp"
            android:layout_marginBottom="@dimen/thirty_dp" />

        <ImageView
            android:id="@+id/datasecurity_image_arrow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_margin="@dimen/ten_dp"
            android:src="@drawable/arrow_list" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/savebattery_image"
        android:layout_width="match_parent"
        android:layout_height="@dimen/zero_dp"
        android:layout_margin="@dimen/five_dp"
        android:layout_weight="1"
        android:background="@drawable/bg_list" >

        <ImageView
            android:layout_width="@dimen/home_image_lit_dimen"
            android:layout_height="wrap_content"
            android:src="@drawable/battery_save_list"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="@dimen/thirty_dp"
            android:layout_marginTop="@dimen/thirty_dp"
            android:layout_marginBottom="@dimen/thirty_dp" />

        <ImageView
            android:id="@+id/savebattery_image_arrow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_margin="@dimen/ten_dp"
            android:src="@drawable/arrow_list" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/applock_image"
        android:layout_width="match_parent"
        android:layout_height="@dimen/zero_dp"
        android:layout_marginBottom="@dimen/ten_dp"
        android:layout_marginLeft="@dimen/five_dp"
        android:layout_marginRight="@dimen/five_dp"
        android:layout_marginTop="@dimen/five_dp"
        android:layout_weight="1"
        android:background="@drawable/bg_list" >

        <ImageView
            android:layout_width="@dimen/home_image_lit_dimen"
            android:layout_height="wrap_content"
            android:src="@drawable/app_lock_list"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="@dimen/thirty_dp"
            android:layout_marginTop="@dimen/thirty_dp"
            android:layout_marginBottom="@dimen/thirty_dp" />

        <ImageView
            android:id="@+id/applock_image_arrow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_margin="@dimen/ten_dp"
            android:src="@drawable/arrow_list" />
    </RelativeLayout>

</LinearLayout>